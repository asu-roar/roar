<launch>
  <!-- Launch Gazebo -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <!-- Load URDF model -->
  <param name="robot_description" command="cat $(find roar_half_sus_description)/urdf/roar_half_sus_description.urdf"/>

  <!-- Spawn robot in Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model roar -param robot_description -x -0.4 -y -0.2 -z 0.5 -Y 1.5708 "/>
  <node name="controller_node" pkg="navigation" type="controller_global.py"/>
  <node name="path_publisher" pkg="navigation" type="path_test.py"/>
  <!-- Spawn controllers -->
  <include file="$(find roar_gazebo)/launch/spawn_controllers.launch">
    <!-- <arg name="robot_ns" value="$(arg robot_ns)" /> -->
  </include>

</launch>
